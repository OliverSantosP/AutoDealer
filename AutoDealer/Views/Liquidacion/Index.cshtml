@model IEnumerable<AutoDealer.Models.Liquidaciones>

@{
    ViewBag.Title = "AutoDealer - Liquidaciones";
}
<div class="widget wblue">
    <div class="widget-head">
        <div class="pull-left">Liquidaciones</div>
        <div class="widget-icons pull-right">
            <a href="#" class="wminimize"><i class="icon-chevron-up"></i></a>
            <a href="#" class="wclose"><i class="icon-remove"></i></a>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="widget-content">
        <div class="table-responsive">
            <table class="table table-bordered ">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Detalle)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.FechaCreacion)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.FechaModificacion)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.MontoVenta)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Personas1.Nombre)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Personas.Nombre)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PagadoCon)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Facturas.NCF)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.Gastos1.PagadoA)
                        </th>
                        <th></th>
                    </tr>
                </thead>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Detalle)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FechaCreacion)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.FechaModificacion)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.MontoVenta)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Personas1.Nombre)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Personas.Nombre)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.PagadoCon)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Facturas.NCF)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Gastos1.PagadoA)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>